    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build:develop": "NODE_ENV=production eslint --fix && mv public/index.dev.html public/index.html && env-cmd -f .env.development react-scripts build",
    "build:prod": "NODE_ENV=production eslint --fix && && mv public/index.prod.html public/index.html && env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
